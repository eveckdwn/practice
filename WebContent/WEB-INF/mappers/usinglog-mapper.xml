<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="usinglog">
	<select id="readUsinglogAll" resultType="hashmap">
		select * from usinglog ORDER BY no ASC
	</select>
	
	<select id="readUsinglogPhone" resultType="hashmap">
		select * from usinglog where phone = #{phone} ORDER BY no ASC
	</select>
</mapper>